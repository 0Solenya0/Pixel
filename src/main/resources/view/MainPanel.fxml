            <VBox alignment="TOP_CENTER" prefHeight="755.0" prefWidth="210.0" style="-fx-background-color: F0F7FF;" BorderPane.alignment="CENTER">
               <effect>
                  <DropShadow />
               </effect>
               <children>
                  <JFXButton fx:id="btnHome" alignment="BASELINE_LEFT" onAction="#showHomePage" prefHeight="55.0" prefWidth="201.0" text="Home">
