                        <Insets top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Laksaman" size="18.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="btnPostTweet" alignment="BASELINE_LEFT" layoutX="10.0" layoutY="30.0" onAction="#showPostTweetPage" prefHeight="55.0" prefWidth="201.0" text="Post tweet">
                     <font>
                        <Font name="Laksaman" size="18.0" />
                     </font>
                  </JFXButton>
               </children>
            </VBox>
         </left>
      </BorderPane>
   </children>
