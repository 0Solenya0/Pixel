<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.LoginController">
   <children>
      <TextField fx:id="txtUsername" layoutX="248.0" layoutY="146.0" promptText="username" />
      <PasswordField fx:id="txtPassword" layoutX="248.0" layoutY="188.0" promptText="password" />
      <Label fx:id="lblUsername" layoutX="164.0" layoutY="150.0" text="Username:" />
      <Label fx:id="lblPassword" layoutX="164.0" layoutY="192.0" text="Password:" />
      <Button fx:id="btnLogin" layoutX="217.0" layoutY="236.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button fx:id="btnRegister" layoutX="303.0" layoutY="236.0" mnemonicParsing="false" onAction="#switchToRegister" text="Register" />
      <Label fx:id="lblErr" alignment="CENTER" layoutX="197.0" layoutY="268.0" prefHeight="16.0" prefWidth="212.0" textFill="RED" />
   </children>
</AnchorPane>
