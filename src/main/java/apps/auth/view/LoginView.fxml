<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="400.0" prefWidth="600.0" type="AnchorPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apps.auth.controller.LoginController">
   <children>
      <TextField fx:id="textUsername" layoutX="248.0" layoutY="146.0" promptText="username" />
      <Label layoutX="164.0" layoutY="150.0" text="Username:" />
      <TextField fx:id="textPassword" layoutX="248.0" layoutY="188.0" promptText="password" />
      <Label layoutX="164.0" layoutY="192.0" text="Password:" />
      <Button fx:id="btnLogin" layoutX="217.0" layoutY="236.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button fx:id="btnRegister" layoutX="303.0" layoutY="236.0" mnemonicParsing="false" text="Register" />
   </children>
</fx:root>
